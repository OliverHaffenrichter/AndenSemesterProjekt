@page
@model AndenSemesterProjekt.Pages.Products.DisplayCategoriesModel
@{
}


@foreach(var item in Model.Categories)
{
<div class="card">
<div class="card-header">
    <h1>@item</h1>
    <div class="card-img">
        <h2>Card Image</h2>
    </div>
    <div class="card-body">
        <h3>Card Body</h3>
    </div>
    <div class="card-footer">
        <h3>Card Footer</h3>
    </div>
</div>
</div>
}